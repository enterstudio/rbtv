{# Header Easteregg #}
<script type="text/javascript">
  var audio = document.getElementById('easteregg_audio');
  var logoglowing = "{{ asset('assets/images/logos/logo-glowing.png') }}";
  var logonormal = "{{ asset('assets/images/logos/logo-menu.png') }}";

  function playeasteregg() {
    if (audio.paused) {
      audio.play();
      document.getElementById("logo").src = logoglowing;
      setTimeout(function () {
        document.getElementById("logo").src = logonormal;
        ;
      }, 47000);
    } else {
      audio.pause();
      audio.currentTime = 0;
      document.getElementById("logo").src = logonormal;
    }
  }
</script>
